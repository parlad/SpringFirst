<!-- Start of Modal for confirm -->
<div class="deleteModal modal fade" id="confirmModal" role="dialog">
	<div class="modal-dialog">
		<!-- Modal content-->
		<form method="post">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4 class="modal-title">confirm</h4>
				</div>
				<div class="modal-body">
					<h5>Do you want to send the mail?</h5>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-raised btn-success"
						id="btnYes">Yes</button>
					<button type="button" class="btn btn-raised btn-warning"
						id="btnNo" data-dismiss="modal">No</button>
				</div>
			</div>
		</form>
	</div>
</div>
<!-- End of Modal for confirm -->
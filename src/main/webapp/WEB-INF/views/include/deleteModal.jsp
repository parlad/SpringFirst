<!-- Start of Modal for Delete -->
<div class="deleteModal modal fade" id="deleteModal" role="dialog">
	<div class="modal-dialog">
		<!-- Modal content-->
		<form method="post">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4 class="modal-title">Delete</h4>
				</div>
				<div class="modal-body">
					<h5>Are you sure to delete?</h5>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-raised btn-success"
						id="btnDelete">Delete</button>
					<button type="button" class="btn btn-raised btn-warning"
						data-dismiss="modal">Cancel</button>
				</div>
			</div>
		</form>
	</div>
</div>
<!-- End of Modal for Delete -->